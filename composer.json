  "license": "MIT",
  "require": {
    "symfony/framework-bundle": "^4.1",
    "symfony/yaml": "^4.1",
    "symfony/orm-pack": "^1.0",
    "rikudou/installer": "^1.5"
  },
  "autoload": {
